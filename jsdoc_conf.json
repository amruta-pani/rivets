{
    "plugins": [
        "plugins/markdown",
        "plugins/summarize"
    ],
    "markdown": {
        "idInHeadings": true,
        "tags": [""]
    },
    "recurseDepth": 10,
    "source": {
        "include": ["index.js", "controllers", "utils"],
        "exclude": [ "node_modules", "logs", "docs", ".vscode", "models" ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "templates": "minami",
        "encoding": "utf8",
        "destination": "./docs/latest/",
        "recurse": true
    }
}